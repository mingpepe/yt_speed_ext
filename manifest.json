{
    "manifest_version": 3,
    "name": "YouTube Speed Controller",
    "version": "1.0",
    "description": "Change YouTube playback speed with keyboard shortcuts",
    "permissions": [
        "activeTab"
    ],
    "content_scripts": [
        {
            "matches": [
                "*://www.youtube.com/*"
            ],
            "js": [
                "content.js"
            ],
            "run_at": "document_start"
        }
    ]
}